name: Second workflow
on: push
jobs:
  first-job:
    runs-on: ubuntu-latest
    steps:
      - name: print greeting
        run: |
            echo "Hello world SecondTimeWorkflow"
            echo "Hello world1 SecondTimeWorkflow"
            
      - name: print goodbye
        run: |
          echo "Done world SecondTimeWorkflow"
          echo "Done world SecondTimeWorkflow"
      
      
    
